		<component id="root_scene">
			<transformation>
			</transformation>

			<materials>
				<material id="plastic" />
			</materials>

			<texture id="none" />

			<children>
				<componentref id="floor" />
				<componentref id="walls" />
				<componentref id="tv" />
				<componentref id="window" />
				<componentref id="sofa" />
				<componentref id="tree" />

				<componentref id="carpet_2" />
				<componentref id="carpet_3" />
				<componentref id="carpet_4" />
			</children>
		</component>

		<!-- ************************* GAME ************************* -->

		<component id="game">
			<transformation>
				<translate x="0" y="1.06" z="0" />
